                    <!-- Layer 1/<Group>/<Path> -->
                    <Path StrokeThickness="0.5" Stroke="#ffb2b2b2" StrokeMiterLimit="1.0" Fill="#ffbbdca4" Data="F1 M 501.379,472.109 C 594.563,364.530 593.966,203.855 499.604,96.948 L 312.911,283.640 L 501.379,472.109 Z" Canvas.Top="58"/>
                    <!-- Layer 1/<Group>/<Path> -->
                    <Path StrokeThickness="0.5" Stroke="#ffb2b2b2" StrokeMiterLimit="1.0" Fill="#fff7a48c" Data="F1 M 73.248,94.932 C -23.492,202.494 -24.074,365.874 71.473,474.124 L 261.956,283.640 L 73.248,94.932 Z" Canvas.Top="58"/>
                    <!-- Layer 1/<Group>/<Path> -->
                    <Path StrokeThickness="0.5" Stroke="#ffb2b2b2" StrokeMiterLimit="1.0" Fill="#fffff0a4" Data="F1 M 96.950,499.601 C 205.200,595.148 368.580,594.566 476.142,497.826 L 287.433,309.118 L 96.950,499.601 Z" Canvas.Top="58"/>
